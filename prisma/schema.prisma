generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PromptBlock {
  id        String   @id @default(cuid())
  type      String
  title     String
  content   String
  tags      String[]
  isTemp    Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
